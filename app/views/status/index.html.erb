	        <!-- /.box-tools -->
	      </div>
	      <!-- /.box-header -->
	      <div class="box-body">
	        <span class="text text-danger">FORA</span>
	      </div>
	      <!-- /.box-body -->
	    </div>
	    <!-- /.box -->
	  </div>
